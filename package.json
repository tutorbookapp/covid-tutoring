  "scripts": {
    "start:db": "firebase emulators:start --project test-tutorbook --only firestore",
    "start:next": "cross-env NODE_OPTIONS='--inspect' next start -p 3000",
    "start:cy": "cross-env NO_COLOR=1 cypress run",
    "start": "cross-env NODE_ENV=test concurrently yarn:start:db yarn:start:next yarn:start:cy",
    "dev:db": "firebase emulators:start --project development-tutorbook --only firestore",
    "dev:next": "cross-env NODE_OPTIONS='--inspect' next dev -p 3000",
    "dev:cy": "cross-env NODE_ENV=development cypress open",
    "dev": "cross-env NODE_ENV=development concurrently yarn:dev:db yarn:dev:next yarn:dev:cy",
    "build:sw": "cd sw && webpack",
    "build:next": "next build",
    "build:analyze": "cross-env ANALYZE=true next build",
    "build": "concurrently yarn:build:sw yarn:build:next",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx .",
    "style": "pretty-quick --staged"
  },
  "keywords": [
    "Tutorbook",
    "COVID-19",
    "Tutoring",
    "Typescript",
    "Next.js",
    "Sass"
  ],
  "files": [
    "src"
